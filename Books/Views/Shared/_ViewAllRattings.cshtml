@model IEnumerable<Rattings>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-4 col-xs-6 border-primary mb-3">
            <div class="card mb-3" style="max-width: 540px;">
                <div class="row g-0">
                    <div class="col-md-12">
                        <div class="card-header text-white bg-info">
                            <p class="card-text">
                                <h5 class="card-title">
                                   
                                    
                                </h5>
                            </p>
                        </div>
                    </div>
                   
                    <div class="col-md-6">
                        <div class="card-body">
                            <p class="card-text"><b>Sao: </b>@item.Star</p>
                            <p class="card-text"><b>Bình luận: </b>@item.Comment</p>


                        </div>
                    </div>
                    
                </div>
            </div>
        </div>
    }
</div>
